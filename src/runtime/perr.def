DEF_PERR(PERR_OUTOFMEM, "[fatal] out of memory at %s:%d.", PERRCLS_FATAL)
DEF_PERR(PERR_SYSTEM,   "[fatal] system error in %s:%s at %s:%d", PERRCLS_FATAL)
DEF_PERR(PERR_INTERNAL, "[fatal] internal error at %s:%d.", PERRCLS_FATAL)
DEF_PERR(PERR_STKOVRFLW,"[fatal] stack overflow at %s:%d.", PERRCLS_FATAL)
DEF_PERR(PERR_BUFOVRFLW,"[fatal] buffer overflow at %s:%d.", PERRCLS_FATAL)
DEF_PERR(PWRN_DESCLIM,  "[warn] number of descripters exceed limits.", PERRCLS_WARN)
DEF_PERR(PWRN_SYSTEM,   "[warn] system warning in %s:%s at %s:%d", PERRCLS_WARN)
